/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    //// Replace this comment with your code.
    Not(in=sel, out=ns);
    And(a=a, b=ns, out=as);
    And(a=b, b=sel, out=bs);
    Or(a=as, b=bs, out=out);
}